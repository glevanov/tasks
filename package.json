{
  "name": "tasks",
  "description": "A simple task manager",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:gh": "vite build --base=/tasks/",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint . --ext .ts,.tsx --fix",
    "lint:stylelint": "stylelint '**/*.css' --fix",
    "new:component": "hygen component new --target component",
    "new:component:ui": "hygen component new --target ui",
    "new:page": "hygen page new",
    "test:unit": "mocha './**/*.test.?(ts|tsx)'",
    "postinstall": "simple-git-hooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glevanov/tasks.git"
  },
  "author": "Gregory Levanov",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.3.1",
    "effector": "22.3.0",
    "effector-react": "22.1.2",
    "nanoid": "4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "wouter": "2.7.5"
  },
  "devDependencies": {
    "@glevanov/eslint-config": "1.0.1",
    "@testing-library/react": "13.3.0",
    "@types/chai": "4.3.1",
    "@types/mocha": "9.1.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "@vitejs/plugin-react": "2.0.0",
    "chai": "4.3.6",
    "eslint": "8.19.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.30.1",
    "global-jsdom": "8.5.0",
    "hygen": "6.2.7",
    "ignore-styles": "5.0.1",
    "jsdom": "20.0.0",
    "mocha": "10.0.0",
    "simple-git-hooks": "2.8.0",
    "stylelint": "14.9.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "26.0.0",
    "stylelint-order": "5.0.0",
    "ts-node": "10.8.2",
    "typescript": "4.7.4",
    "vite": "3.0.0"
  }
}
